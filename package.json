        "factoid",
        "factoidd",
        "factoshi"
    ],
    "author": "Alex Carrithers",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Factoshi/factoidd/issues"
    },
    "homepage": "https://github.com/Factoshi/factoidd#readme",
    "dependencies": {
        "@hapi/joi": "^17.1.1",
        "axios": "^0.21.1",
        "axios-retry": "^3.1.9",
        "bottleneck": "^2.19.5",
        "dotenv": "^10.0.0",
        "factom": "^1.4.2",
        "joi-factom": "^1.1.2",
        "js-yaml": "^4.1.0",
        "winston": "^3.3.3"
    },
    "devDependencies": {
        "@commitlint/cli": "^12.1.4",
        "@commitlint/config-conventional": "^12.1.4",
        "@types/hapi__joi": "^17.1.7",
        "@types/js-yaml": "^4.0.2",
        "@types/node": "^13.13.52",
        "husky": "^4.3.8",
        "typescript": "^3.9.10"
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
